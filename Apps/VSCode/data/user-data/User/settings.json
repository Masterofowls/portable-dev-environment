{
    "editor.fontSize": 14,
    "editor.fontFamily": "'CaskaydiaCove Nerd Font', 'JetBrainsMono Nerd Font', Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.tabSize": 4,
    "editor.wordWrap": "on",
    "editor.minimap.enabled": true,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": false,
    "editor.suggestSelection": "first",
    "editor.quickSuggestions": {
        "strings": true
    },
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 1000,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/node_modules": false,
        "**/__pycache__": true,
        "**/*.pyc": true
    },
    "window.titleBarStyle": "custom",
    "window.title": "${dirty}${activeEditorShort}${separator}${rootName}${separator}PORTABLE",
    "workbench.startupEditor": "none",
    "workbench.enableExperiments": false,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "terminal.integrated.defaultProfile.windows": "Portable PowerShell",
    "terminal.integrated.fontFamily": "'CaskaydiaCove Nerd Font', 'JetBrainsMono Nerd Font', Consolas",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.profiles.windows": {
        "Portable PowerShell": {
            "path": "F:\\PortableDev\\Apps\\PowerShell\\pwsh.exe",
            "args": [
                "-NoLogo",
                "-NoExit",
                "-ExecutionPolicy",
                "Bypass",
                "-File",
                "F:\\PortableDev\\Config\\PortableProfile.ps1"
            ],
            "icon": "terminal-powershell"
        },
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\System32\\cmd.exe"
            ],
            "icon": "terminal-cmd"
        }
    },
    "terminal.integrated.env.windows": {
        "PORTABLE_ROOT": "F:\\PortableDev",
        "PORTABLE_HOME": "F:\\PortableDev\\Home",
        "HOME": "F:\\PortableDev\\Home",
        "TEMP": "F:\\PortableDev\\Data\\temp",
        "TMP": "F:\\PortableDev\\Data\\temp"
    },
    "git.path": "F:\\PortableDev\\Apps\\Git\\cmd\\git.exe",
    "git.autofetch": false,
    "git.enableSmartCommit": true,
    "python.defaultInterpreterPath": "F:\\PortableDev\\Apps\\Python\\python.exe",
    "python.terminal.activateEnvironment": false,
    "python.analysis.autoSearchPaths": false,
    "python.analysis.indexing": false,
    "npm.packageManager": "npm",
    "telemetry.telemetryLevel": "off",
    "extensions.autoUpdate": false,
    "extensions.autoCheckUpdates": false,
    "update.mode": "none",
    "update.showReleaseNotes": false,
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "**/dist": true,
        "**/build": true,
        "**/__pycache__": true
    },
    "search.useIgnoreFiles": true,
    "search.followSymlinks": false,
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/.hg/store/**": true,
        "**/dist/**": true,
        "**/build/**": true
    },
    "remote.autoForwardPorts": false,
    "security.workspace.trust.enabled": true,
    "security.workspace.trust.untrustedFiles": "prompt",
    "http.proxyStrictSSL": true,
    "liveServer.settings.CustomBrowser": "firefox",
    "liveServer.settings.AdvanceCustomBrowserCmdLine": "${env:PORTABLE_FIREFOX}\\firefox.exe",
    "browser-preview.chromeExecutable": "${env:PORTABLE_FIREFOX}\\firefox.exe",
    "open-in-browser.default": "firefox",
    "git.openRepositoryInParentFolders": "never",
    "chat.mcp.gallery.enabled": true,
    "github.copilot.nextEditSuggestions.enabled": true,
    "chat.instructionsFilesLocations": {
        ".github/instructions": true,
        "F:\\PortableDev\\Apps\\VSCode\\data\\tmp\\postman-collections-post-response.instructions.md": true,
        "F:\\PortableDev\\Apps\\VSCode\\data\\tmp\\postman-collections-pre-request.instructions.md": true,
        "F:\\PortableDev\\Apps\\VSCode\\data\\tmp\\postman-folder-post-response.instructions.md": true,
        "F:\\PortableDev\\Apps\\VSCode\\data\\tmp\\postman-folder-pre-request.instructions.md": true,
        "F:\\PortableDev\\Apps\\VSCode\\data\\tmp\\postman-http-request-post-response.instructions.md": true,
        "F:\\PortableDev\\Apps\\VSCode\\data\\tmp\\postman-http-request-pre-request.instructions.md": true
    },
    "workbench.productIconTheme": "fluent-icons",
    "RainbowBrackets.depreciation-notice": false,
    "terminal.integrated.shellIntegration.environmentReporting": true,
    "terminal.integrated.sendKeybindingsToShell": true,
    "terminal.integrated.allowMnemonics": true,
    "terminal.integrated.suggest.enabled": true,
    "notes.notesLocation": "f:\\PortableDev\\Notes",
    "vscode_custom_css.imports": [
        "file:///f:/PortableDev/Apps/VSCode/data/extensions/brandonkirbyson.vscode-animations-2.0.7/dist/updateHandler.js"
    ],
    "editor.gotoLocation.multipleDefinitions": "goto"
}